{
  "name": "assessmentbot-backend",
  "version": "0.0.1",
  "description": "The backend for Assessment Bot, which provides LLM services.",
  "author": "Hamish Arnold",
  "private": true,
  "license": "ISC",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "debug": "node --inspect-brk ./node_modules/.bin/nest start",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:british": "./scripts/check-british-english.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run build && jest --config ./jest-e2e.config.cjs --runInBand",
    "verify:assessor": "ts-node scripts/verify-assessor.ts",
    "test:pentest": "node scripts/pentest-all.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/genai": "^1.9.0",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.4",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "dotenv": "^17.2.0",
    "file-type": "^21.0.0",
    "jsonrepair": "^3.13.0",
    "mime-detect": "^1.3.0",
    "mustache": "^4.2.0",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pino-http": "^10.5.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "validator": "^13.15.15",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.8",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mustache": "^4.2.6",
    "@types/node": "^24.1.0",
    "@types/passport": "^1.0.17",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.37.0",
    "axios": "^1.10.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "lint-staged": "^16.1.2",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  },
  "homepage": "https://github.com/h-arnold/AssessmentBot-Backend#readme",
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.{js,ts,json,md}": "prettier --write",
    "*.{ts,js}": "./scripts/check-british-english.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-arnold/AssessmentBot-Backend.git"
  },
  "bugs": {
    "url": "https://github.com/h-arnold/AssessmentBot-Backend/issues"
  }
}
