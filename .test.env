# Test environment variables
# This file is used by Jest tests and should mirror production environment structure

NODE_ENV=test
PORT=3000
APP_NAME=AssessmentBot-Backend
APP_VERSION=1.0.0
DATABASE_URL=test_database_url

# API Keys for testing - using test values
API_KEYS=test_key_1,test_key_2

# Image Upload Limits
MAX_IMAGE_UPLOAD_SIZE_MB=1
ALLOWED_IMAGE_MIME_TYPES=image/png,image/jpeg

# Gemini API Key - will be populated from GitHub secrets in CI
GEMINI_API_KEY=test_gemini_key

# Log Level for tests
LOG_LEVEL=error

# Throttler (Rate Limiting) Settings for tests
THROTTLER_TTL=60
UNAUTHENTICATED_THROTTLER_LIMIT=5
AUTHENTICATED_THROTTLER_LIMIT=10